<table>
  <thead>
  <th>name</th>
  <th>time</th>
  <th>date</th>
  </thead>
  <tbody>
  <td>
<% @practices.each do |practice| %>
  <%= link_to "#{practice.name}", practice %>
  </td>
    <td>
  <%= practice.hours %>h
    </td>
      <td>
  <%= practice.created_at.strftime("%Y-%m-%d").downcase %>
      </td>
<% end %>
  </tbody>
</table>