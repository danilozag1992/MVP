<h1>
  <%= @practice.name %>
  <h1>Duration:</h1>
  <%= @practice.hours %>
  <h4>Starting time:</h4>
  <%= @practice.start_time %>
  <h4>Ending Time</h4>
  <%= @practice.end_time %>
</h1>
<% if current_user %>
<%= button_to 'edit', edit_practice_path(@practice), method: :get, class: "btn btn-primary btn-lg btn-block" %>
<%= button_to 'Destroy', @practice, method: :delete,class: "btn btn-primary btn-lg btn-block" , data: { confirm: 'Are you sure?' } %>
<% end %>