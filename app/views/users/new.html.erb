<h1>Sign up!</h1>
<div>
  <%= form_with(model: @user, local: true) do |f| %>
    <div>
    <%= f.file_field :image %>
    </div>
    <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </div>
    <%= f.submit "Create my account" %>
  <% end %>
  <p>If you already have account, please <%= link_to 'Login', login_path  %></p>
</div>
