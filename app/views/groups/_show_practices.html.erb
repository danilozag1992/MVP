<table class="table">
  <thead>
  <tr>
    <th scope="col">Practice</th>
    <th scope="col">Duration</th>
    <th scope="col">Created</th>
    <th scope="col">Coach</th>
  </tr>
  </thead>
  <tbody>
  <% practices.each do |practice| %>
    <tr>
      <td><%= link_to "#{practice.name}", practice %></td>
      <td><%= practice.hours %> hs</td>
      <td><%= practice.created_at.strftime("%b %d, %Y").downcase %></td>
      <td><%= User.find_by(id: practice.coach_id).name %></td>
    </tr>
  <% end %>
  </tbody>
</table>